{
  "name": "@proc7ts/delta-set",
  "version": "2.0.0-pre.0",
  "description": "JavaScript Set keeping delta of changes made to it",
  "keywords": [
    "delta",
    "set"
  ],
  "homepage": "https://github.com/proc7ts/delta-set",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:proc7ts/delta-set.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/proc7ts/delta-set/issues"
  },
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "default": "./dist/delta-set.js"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.3",
    "@run-z/eslint-config": "^3.2.2",
    "@types/node": "^16.11.60",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.24.0",
    "eslint-plugin-jest": "^27.0.4",
    "gh-pages": "^4.0.0",
    "jest": "^29.0.3",
    "jest-junit": "^14.0.1",
    "prettier": "^2.7.1",
    "prettier-eslint-cli": "^7.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-flat-dts": "^1.7.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.0",
    "run-z": "^1.10.1",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup -c",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf \"index.d.ts?(.map)\" dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "format": "run-z +z --then prettier-eslint --write --include-dot-files \"src/**/*.*\" \"*.{js,cjs,mjs,json,md}\"",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z env:NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" --then jest",
    "z": "run-z +cmd:rollup,+cmd:typedoc,+cmd:eslint,+cmd:jest"
  }
}
